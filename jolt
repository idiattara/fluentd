def resultList = []

    // Parcourir les objets JSON et extraire les valeurs
    jsonArray.each { jsonObj ->
        if (jsonObj.containsKey("compo")) {
            def compoValue = jsonObj.compo
            def matches = (compoValue =~ /%current_(.*?)\|/)

            if (matches) {
                matches.each { match ->
                    // Ajouter la valeur correspondante Ã  la liste
                    resultList.add(match[1])
                }
            }
        }
    
