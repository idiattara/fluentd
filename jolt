
    // Lire le contenu du flowfile
    def flowFileInputStream = session.read(flowFile)
    def jsonContent = flowFileInputStream.text
    flowFileInputStream.close()

    // Parsez le contenu du flowfile en tant que JSON
    def jsonSlurper = new groovy.json.JsonSlurper()
    def parsedJsonContent = jsonSlurper.parseText(jsonContent)
