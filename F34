import json

# Charger le JSON depuis un fichier
with open('votre_fichier.json', 'r') as fichier_json:
    data = json.load(fichier_json)

# Extraire le contenu de la clé "metadata"
metadata = data.get('metadata', {})

# Écrire le résultat dans un fichier
with open('resultat_metadata.txt', 'w') as fichier_resultat:
    fichier_resultat.write(json.dumps(metadata, indent=2))

print("Le contenu de metadata a été extrait et écrit dans 'resultat_metadata.txt'")
